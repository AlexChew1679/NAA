<%= provide(:title, @user.last_name) %>

<div class="jumbotron">

    <h1 class = "center"><%= @user.first_name + ' ' + @user.last_name %></h1>
  <h2 class = "center">  <%= link_to 'Edit', edit_user_path %></h2>
  <h2> <%= link_to 'Go to Todos', new_task_path %> </h2>

</div>



<div id="edit">
	<%= form_for @user do |f| %>
	    <p>
	      <%= f.label :first_name %>
        <%= f.text_field :first_name %>
	    </p>

	    <p>
	      <%= f.label :last_name %>
        <%= f.text_field :last_name %>
	    </p>

	    <p>
	      <%= f.submit %>
	    </p>
	  <% end %>
</div>
	<button id="pro">Edit Profile</button>
<br>
